<view><cu-custom vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">我的收藏</view></cu-custom><block wx:for="{{postList}}" wx:for-item="list" wx:for-index="index" wx:key="index"><view class="postList"><view class="user"><view data-event-opts="{{[['tap',[['getbieren',['$0'],[[['postList','',index,'__$n0.AuthorId']]]]]]]}}" class="touxiang" style="{{'background-image:'+('url('+list[0].AuthorInfo.AuthorAvatarUrl+')')+';'}}" bindtap="__e"></view><view class="userName">{{list[0].AuthorInfo.AuthorName}}</view><view class="postDate">{{list[0].PsotDate}}</view></view><view data-event-opts="{{[['tap',[['getpostinfo',['$0'],[[['postList','',index,'__$n0.PostsId']]]]]]]}}" class="postText" bindtap="__e">{{''+list[0].Content+''}}</view><view class="postImage"><block wx:for="{{list[0].PictureId}}" wx:for-item="img" wx:for-index="id" wx:key="id"><image class="postImageItem" src="{{img}}" mode="aspectFill" data-event-opts="{{[['tap',[['showImage',['$0',id],[[['postList','',index,'__$n0.PictureId']]]]]]]}}" bindtap="__e"></image></block></view><view class="postBottom"><view data-event-opts="{{[['tap',[['getClass',['$0'],[[['postList','',index,'__$n0.Tag.ClassId']]]]]]]}}" class="postClass" bindtap="__e">{{"#"+list[0].Tag.ClassName}}</view><view data-event-opts="{{[['tap',[['Like',['$0','$1','$2',index],[[['postList','',index,'__$n0.PostsId']],[['postList','',index,'__$n0.AuthorId']],[['postList','',index,'__$n0.Give']]]]]]]}}" class="{{[list[0].Give?'likeing':'like']}}" bindtap="__e"></view><view data-event-opts="{{[['tap',[['caidan',['$event']]]]]}}" class="postMenu" bindtap="__e"><image style="height:40rpx;" src="../../static/caidan.png" mode="aspectFit"></image></view></view></view></block></view>