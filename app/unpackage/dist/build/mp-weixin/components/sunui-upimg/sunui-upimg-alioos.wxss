/* 预览图片 */
.sunsin_picture_item.data-v-141dc053 {
	position: relative;
	margin: 20rpx 21rpx 0 0;
}
/* 删除按钮样式 */
.sunsin_picture_item .del.data-v-141dc053 {
	position: absolute;
	color: #fff;
	border-radius: -4rpx;
	border-top-right-radius: 6rpx;
	width: 40rpx;
	height: 40rpx;
	line-height: 40rpx;
	z-index: 2;
	text-align: center;
	background-color: #E54D42;
}
/* 删除图标位置(上左) */
.sunsin_picture_item .del.left.data-v-141dc053 {
	top: 0;
	left: 0;
	margin-left: 4%;
	border-top-right-radius: 0;
	border-top-left-radius: 6rpx;
}
/* 删除图标位置(上右) */
.sunsin_picture_item .del.right.data-v-141dc053 {
	top: 0;
	right: -4.2%;
}
/* 删除图标位置(下左) */
.sunsin_picture_item .del.bleft.data-v-141dc053 {
	bottom: 0;
	left: 4%;
	border-top-left-radius: 0;
	border-bottom-left-radius: 6rpx;
}
/* 删除图标位置(下右) */
.sunsin_picture_item .del.bright.data-v-141dc053 {
	right: -4.2%;
	bottom: 0;
	border-top-right-radius: 0;
}
/* 进度遮罩层样式 */
.sunsin_upload_progress.data-v-141dc053 {
	position: absolute;
	top: 0;
	left: 0;
	color: #fff;
	font-size: 24rpx;
	text-align: center;
	line-height: 160rpx;
