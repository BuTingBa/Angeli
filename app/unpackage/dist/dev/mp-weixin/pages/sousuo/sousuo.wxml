<view><cu-custom vue-id="1" bgColor="bg-white" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">搜索</view></cu-custom><view class="cu-bar bg-white search fixed" style="box-shadow:0 0rpx 0rpx rgba(0, 0, 0, 0.1);" id="topbox"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" focus="true" placeholder="搜索帖子/用户" confirm-type="search" data-event-opts="{{[['input',[['input',['$event']]]],['confirm',[['sososo',['$event']]]]]}}" bindinput="__e" bindconfirm="__e"/></view><view data-event-opts="{{[['tap',[['sososo',['$event']]]]]}}" class="action anniu" bindtap="__e">搜索</view></view><view style="{{'height:'+(gaodu+'px')+';'}}"></view><block wx:if="{{home==true}}"><block><view class="Top"><view class="TopName">话题分类</view><scroll-view class="lunbo " scroll-x="true" data-event-opts="{{[['scroll',[['log',['$event']]]]]}}" bindscroll="__e"><view class="bianju"></view><block wx:for="{{classList}}" wx:for-item="typea" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getPOST',['$0'],[[['classList','',index,'ClassId']]]]]]]}}" class="lunboItem radius shadow-blur " style="{{'background-image:'+('url('+typea.ClassImage+')')+';'}}" bindtap="__e"><view class="text-c">{{typea.ClassName}}</view></view></block><view class="bianju"></view></scroll-view></view><view class="tuijianBox"><view class="TopName">每日推荐</view><block wx:for="{{hotList}}" wx:for-item="list" wx:for-index="index" wx:key="index"><view class="tuijian"><view class="userinfo"><view data-event-opts="{{[['tap',[['getbieren',['$0'],[[['hotList','',index,'AuId.Auid']]]]]]]}}" class="touxiang" style="{{'background-image:'+('url('+list.AuId.AuthorAvatarUrl+')')+';'}}" bindtap="__e"></view><view class="name">{{list.AuId.AuthorName}}</view><view data-event-opts="{{[['tap',[['adduser',['$0',index],[[['hotList','',index,'AuId.Auid']]]]]]]}}" class="guanzhuuser" bindtap="__e"><view class="{{[list.AuId.isGz?'anniuguanzhuXZ':'anniuguanzhu']}}">{{list.AuId.isGz?'取消关注':'+关注'}}</view></view><view class="zhongcao">{{list.AuId.Auid}}</view></view><view class="miaoshu">{{list.AuId.ms}}</view></view></block></view></block></block><block wx:if="{{home==false}}"><block><view class="daohang"><view data-event-opts="{{[['tap',[['tabSelect',[0]]]]]}}" class="{{['hang',TabCur==0?'hangIng':'']}}" bindtap="__e">帖子</view><view data-event-opts="{{[['tap',[['tabSelect',[1]]]]]}}" class="{{['hang',TabCur==1?'hangIng':'']}}" bindtap="__e">用户</view></view><block wx:if="{{TabCur==0}}"><block><block wx:if="{{weikong==false}}"><view class="postBox"><block wx:for="{{postList}}" wx:for-item="list" wx:for-index="index" wx:key="index"><view class="postList"><view class="user"><view data-event-opts="{{[['tap',[['getbieren',['$0'],[[['postList','',index,'AuthorId']]]]]]]}}" class="touxiang" style="{{'background-image:'+('url('+list.AuthorInfo.AuthorAvatarUrl+')')+';'}}" bindtap="__e"></view><view class="userName">{{list.AuthorInfo.AuthorName}}</view><view class="postDate">{{list.PsotDate}}</view></view><view data-event-opts="{{[['tap',[['getpostinfo',['$0'],[[['postList','',index,'PostsId']]]]]]]}}" class="postText" bindtap="__e"><text>{{list.Content}}</text></view><block wx:if="{{list.PictureId[0].length>5}}"><block><view class="postImage"><block wx:for="{{list.PictureId}}" wx:for-item="img" wx:for-index="id" wx:key="id"><image class="postImageItem" src="{{img}}" mode="aspectFill" data-event-opts="{{[['tap',[['showImage',['$0',id],[[['postList','',index,'PictureId']]]]]]]}}" bindtap="__e"></image></block></view></block></block><view class="postBottom"><view data-event-opts="{{[['tap',[['getClass',['$0'],[[['postList','',index,'Tag.ClassId']]]]]]]}}" class="postClass" bindtap="__e">{{"#"+list.Tag.ClassName}}</view><view data-event-opts="{{[['tap',[['Like',['$0'],[[['postList','',index,'PostsId']]]]]]]}}" class="like" bindtap="__e"></view><view data-event-opts="{{[['tap',[['caidan',['$0'],[[['postList','',index]]]]]]]}}" class="postMenu" bindtap="__e"><image style="height:40rpx;" src="../../static/caidan.png" mode="aspectFit"></image></view></view></view></block><uni-load-more vue-id="2" status="{{status}}" bind:__l="__l"></uni-load-more></view></block><block wx:else><view class="nullBox"><image class="nullImg" src="../../static/null.png" mode></image><text class="nullText">\n没有搜索到相关帖子</text></view></block></block></block><block wx:if="{{TabCur==1}}"><block><block wx:if="{{weikong==false}}"><block wx:for="{{userList}}" wx:for-item="user" wx:for-index="id" wx:key="id"><view class="userbox"><view class="userGuanzhu"><view data-event-opts="{{[['tap',[['getbieren',['$0'],[[['userList','',id,'Auid']]]]]]]}}" class="touxiang" style="{{'background-image:'+('url('+user.AvatarUrl+')')+';'}}" bindtap="__e"></view><view class="userName">{{user.name}}</view><view class="guanzhuanniu">关注</view><view class="usermiaoshu">{{user.miaoshu}}</view></view><view style="height:1rpx;background-color:#F7F8FA;"></view></view></block></block><block wx:else><view class="nullBox"><image class="nullImg" src="../../static/null.png" mode></image><text class="nullText">\n没有搜索到相关用户</text></view></block></block></block></block></block></view>